{
  "name": "@loopback/example-webpack",
  "version": "0.0.1",
  "description": "An example to bundle @loopback/core using webpack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:webpack": "webpack --silent --config webpack-web.config.js && webpack --silent --config webpack-node.config.js",
    "build:webpack-node": "webpack --silent --config webpack-node.config.js",
    "clean": "lb-clean *example-webpack*.tgz dist *.tsbuildinfo package",
    "verify": "npm pack && tar xf *example-webpack*.tgz && tree package && npm run clean",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "prestart": "npm run build",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/webpack"
  },
  "dependencies": {
    "@loopback/core": "^2.9.5",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.2",
    "@loopback/eslint-config": "^9.0.2",
    "@loopback/testlab": "^3.2.4",
    "@types/node": "^10.17.32",
    "eslint": "^7.9.0",
    "ts-loader": "^8.0.3",
    "typescript": "~4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial"
  ]
}
