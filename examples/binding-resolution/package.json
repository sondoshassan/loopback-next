{
  "name": "@loopback/example-binding-resolution",
  "version": "0.0.1",
  "description": "An example application to demonstrate binding resolution and dependency injection within a context hierarchy",
  "keywords": [
    "loopback-application",
    "loopback",
    "IoC",
    "dependency injection",
    "context",
    "binding"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/binding-resolution"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.5.1",
    "@loopback/core": "^2.9.5",
    "@loopback/rest": "^6.2.0",
    "@loopback/rest-explorer": "^2.2.10",
    "debug": "^4.1.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.2",
    "@loopback/testlab": "^3.2.4",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.28",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.2"
  },
  "copyright.owner": "IBM Corp."
}
